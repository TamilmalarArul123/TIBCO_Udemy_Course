<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/ListReadRequest" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/ListReadResponse" xmlns:ns="http://www.tibco.com/schemas/2.ParseRenderXML/Resources/Schemas/Models/Header" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns3="http://www.tibco.com/schemas/2.ParseRenderXML/Resources/Schemas/Commons/Response">
    <xsd:import namespace="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/ListReadRequest" schemaLocation="/Resources/Schemas/Services/ListReadRequest.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/ListReadResponse" schemaLocation="/Resources/Schemas/Services/ListReadResponse.xsd"/>
    <pd:name>Processes/Services/SharedFolders/ListReadFiles.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:ListReadRequest"/>
    <pd:startX>49</pd:startX>
    <pd:startY>200</pd:startY>
    <pd:returnBindings>
        <pfx2:ListReadResponse>
            <xsl:copy-of select="$Start/pfx:ListReadRequest/ns:Header"/>
            <pfx2:Body>
                <ns3:Response>
                    <ns3:message>
                        <xsl:value-of select="'list and read files done'"/>
                    </ns3:message>
                    <ns3:responseStatus>
                        <xsl:value-of select="'SUCCESS'"/>
                    </ns3:responseStatus>
                    <ns3:data>
                        <xsl:copy-of select="$Map-Data/Files"/>
                    </ns3:data>
                </ns3:Response>
            </pfx2:Body>
        </pfx2:ListReadResponse>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:ListReadResponse"/>
    <pd:endX>922</pd:endX>
    <pd:endY>202</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1763703521163</pd:targetNamespace>
    <pd:activity name="List Files">
        <pd:type>com.tibco.plugin.file.ListFilesActivity</pd:type>
        <pd:resourceType>ae.activities.ListFilesActivity</pd:resourceType>
        <pd:x>168</pd:x>
        <pd:y>202</pd:y>
        <config>
            <mode>files-and-directories</mode>
        </config>
        <pd:inputBindings>
            <ns2:ListFilesActivityConfig>
                <fileName>
                    <xsl:value-of select="concat($Start/pfx:ListReadRequest/pfx:Body/pfx:source/pfx:sourcePath,$Start/pfx:ListReadRequest/pfx:Body/pfx:source/pfx:sourceFilePattern)"/>
                </fileName>
            </ns2:ListFilesActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>296</pd:x>
        <pd:y>199</pd:y>
        <config>
            <element>
                <xsd:element name="Files">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="fileinfo" minOccurs="0" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="fullName" type="xsd:string"/>
                                        <xsd:element name="fileName" type="xsd:string"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <Files>
                <xsl:for-each select="$List-Files/ns2:ListFilesActivityOutput/files/fileInfo[contains(fileName, '.txt') and starts-with(fileName,'file_')]">
                    <fileinfo>
                        <fullName>
                            <xsl:value-of select="fullName"/>
                        </fullName>
                        <fileName>
                            <xsl:value-of select="fileName"/>
                        </fileName>
                    </fileinfo>
                </xsl:for-each>
            </Files>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>366</pd:x>
        <pd:y>19</pd:y>
        <pd:width>484</pd:width>
        <pd:height>362</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Map-Data/Files/fileinfo</pd:over>
            <pd:iterationElementSlot>currentFile</pd:iterationElementSlot>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Read File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>RemoveFiles</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>MoveFiles</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>CopyFiles</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Null</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>One Bend</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>CopyFiles</pd:to>
            <pd:xpathDescription>file content equals 1</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$Read-File/ns2:ReadActivityOutputTextClass/fileContent/textContent='1'</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>MoveFiles</pd:to>
            <pd:xpathDescription>file content equals 2</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$Read-File/ns2:ReadActivityOutputTextClass/fileContent/textContent='2'</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>Null</pd:to>
            <pd:xpathDescription>Do nothing</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>RemoveFiles</pd:to>
            <pd:xpathDescription>file content equals 3</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$Read-File/ns2:ReadActivityOutputTextClass/fileContent/textContent='3'</pd:xpath>
        </pd:transition>
        <pd:activity name="Read File">
            <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
            <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
            <pd:x>478</pd:x>
            <pd:y>184</pd:y>
            <config>
                <encoding>text</encoding>
            </config>
            <pd:inputBindings>
                <ns0:ReadActivityInputClass xmlns:ns0="http://www.tibco.com/namespaces/tnt/plugins/file/v2">
                    <fileName>
                        <xsl:value-of select="$currentFile/fileinfo/fullName"/>
                    </fileName>
                </ns0:ReadActivityInputClass>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="CopyFiles">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>658</pd:x>
            <pd:y>82</pd:y>
            <config>
                <processName>/Processes/Services/SharedFolders/CopyFiles.process</processName>
            </config>
            <pd:inputBindings>
                <ns0:CopyFileRequest xmlns:ns0="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/CopyFileRequest">
                    <xsl:copy-of select="$Start/pfx:ListReadRequest/ns:Header"/>
                    <ns0:Body>
                        <ns0:source>
                            <ns0:sourceDirectoryPath>
                                <xsl:value-of select="$Start/pfx:ListReadRequest/pfx:Body/pfx:source/pfx:sourcePath"/>
                            </ns0:sourceDirectoryPath>
                            <ns0:sourceFileName>
                                <xsl:value-of select="$currentFile/fileinfo/fileName"/>
                            </ns0:sourceFileName>
                        </ns0:source>
                        <ns0:target>
                            <ns0:targetDirectoryPath>
                                <xsl:value-of select="$Start/pfx:ListReadRequest/pfx:Body/pfx:target/pfx:targetPath"/>
                            </ns0:targetDirectoryPath>
                            <ns0:targetFileName>
                                <xsl:value-of select="concat('final_',$currentFile/fileinfo/fileName)"/>
                            </ns0:targetFileName>
                        </ns0:target>
                    </ns0:Body>
                </ns0:CopyFileRequest>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="MoveFiles">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>667</pd:x>
            <pd:y>190</pd:y>
            <config>
                <processName>/Processes/Services/SharedFolders/MoveFiles.process</processName>
            </config>
            <pd:inputBindings>
                <ns0:MoveFilesRequest xmlns:ns0="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/MoveFilesRequest">
                    <xsl:copy-of select="$Start/pfx:ListReadRequest/ns:Header"/>
                    <ns0:Body>
                        <ns0:source>
                            <ns0:sourceDirectoryPath>
                                <xsl:value-of select="$Start/pfx:ListReadRequest/pfx:Body/pfx:source/pfx:sourcePath"/>
                            </ns0:sourceDirectoryPath>
                            <ns0:sourceFileName>
                                <xsl:value-of select="$currentFile/fileinfo/fileName"/>
                            </ns0:sourceFileName>
                        </ns0:source>
                        <ns0:target>
                            <ns0:targetDirectoryPath>
                                <xsl:value-of select="$Start/pfx:ListReadRequest/pfx:Body/pfx:target/pfx:targetPath"/>
                            </ns0:targetDirectoryPath>
                            <ns0:targetFileName>
                                <xsl:value-of select="concat('file2_moved_',$currentFile/fileinfo/fileName)"/>
                            </ns0:targetFileName>
                        </ns0:target>
                    </ns0:Body>
                </ns0:MoveFilesRequest>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Null">
            <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
            <pd:resourceType>ae.activities.null</pd:resourceType>
            <pd:x>479</pd:x>
            <pd:y>320</pd:y>
            <config/>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="RemoveFiles">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>602</pd:x>
            <pd:y>256</pd:y>
            <config>
                <processName>/Processes/Services/SharedFolders/RemoveFiles.process</processName>
            </config>
            <pd:inputBindings>
                <ns0:RemoveFilesRequest xmlns:ns0="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/RemoveFilesRequest">
                    <xsl:copy-of select="$Start/pfx:ListReadRequest/ns:Header"/>
                    <ns0:Body>
                        <ns0:source>
                            <ns0:sourceDirectoryPath>
                                <xsl:value-of select="$Start/pfx:ListReadRequest/pfx:Body/pfx:source/pfx:sourcePath"/>
                            </ns0:sourceDirectoryPath>
                            <ns0:sourceFileName>
                                <xsl:value-of select="$currentFile/fileinfo/fileName"/>
                            </ns0:sourceFileName>
                        </ns0:source>
                    </ns0:Body>
                </ns0:RemoveFilesRequest>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>List Files</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>List Files</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>