<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:ns="http://www.tibco.com/schemas/2.ParseRenderXML/Resources/Schemas/Models/Users" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns2="http://www.tibco.com/namespaces/tnt/plugins/file">
    <xsd:import namespace="http://www.tibco.com/schemas/2.ParseRenderXML/Resources/Schemas/Models/Users" schemaLocation="/Resources/Schemas/Models/Users.xsd"/>
    <pd:name>Processes/Services/SharedFolders/CreateCSVFiles.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startX>73</pd:startX>
    <pd:startY>104</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>881</pd:endX>
    <pd:endY>111</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1763739209772</pd:targetNamespace>
    <pd:activity name="Create Header Data">
        <pd:type>com.tibco.plugin.parse.RenderActivity</pd:type>
        <pd:resourceType>ae.activities.RenderActivity</pd:resourceType>
        <pd:x>221</pd:x>
        <pd:y>103</pd:y>
        <config>
            <ParseSharedConfig>/Resources/CSV/Data Format.sharedparse</ParseSharedConfig>
        </config>
        <pd:inputBindings>
            <Rows>
                <ns:Users>
                    <ns:id>
                        <xsl:value-of select="concat('&quot;', 'ID', ' &quot;')"/>
                    </ns:id>
                    <ns:firstName>
                        <xsl:value-of select="concat('&quot;', 'FIRSTNAME', ' &quot;')"/>
                    </ns:firstName>
                    <ns:lastName>
                        <xsl:value-of select="concat('&quot; ', 'LASTNAME', ' &quot;')"/>
                    </ns:lastName>
                    <ns:nationality>
                        <xsl:value-of select="concat('&quot; ', 'NATIONALITY', ' &quot;')"/>
                    </ns:nationality>
                    <ns:department>
                        <xsl:value-of select="concat('&quot; ', 'DEPARTMENT', ' &quot;')"/>
                    </ns:department>
                </ns:Users>
            </Rows>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Create CSV file with Header">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>365</pd:x>
        <pd:y>108</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
        </config>
        <pd:inputBindings>
            <ns2:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="'C:\Users\tamilmar\Documents\TIBCO_Udemy_Course\Files\Input\users.csv'"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="$Create-Header-Data/Text"/>
                </textContent>
            </ns2:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Sample User Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>476</pd:x>
        <pd:y>105</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="ns:Users" minOccurs="0" maxOccurs="unbounded"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <root>
                <ns:Users>
                    <ns:id>
                        <xsl:value-of select="'1'"/>
                    </ns:id>
                    <ns:firstName>
                        <xsl:value-of select="'tamil'"/>
                    </ns:firstName>
                    <ns:lastName>
                        <xsl:value-of select="'arul'"/>
                    </ns:lastName>
                    <ns:nationality>
                        <xsl:value-of select="'indian'"/>
                    </ns:nationality>
                    <ns:department>
                        <xsl:value-of select="'ece'"/>
                    </ns:department>
                </ns:Users>
                <ns:Users>
                    <ns:id>
                        <xsl:value-of select="'2'"/>
                    </ns:id>
                    <ns:firstName>
                        <xsl:value-of select="'malar'"/>
                    </ns:firstName>
                    <ns:lastName>
                        <xsl:value-of select="'arul'"/>
                    </ns:lastName>
                    <ns:nationality>
                        <xsl:value-of select="'indian'"/>
                    </ns:nationality>
                    <ns:department>
                        <xsl:value-of select="'eee'"/>
                    </ns:department>
                </ns:Users>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>555</pd:x>
        <pd:y>38</pd:y>
        <pd:width>256</pd:width>
        <pd:height>142</pd:height>
        <pd:collapsedWidth>52</pd:collapsedWidth>
        <pd:collapsedHeight>69</pd:collapsedHeight>
        <pd:groupVariables/>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Sample-User-Data/root/ns:Users</pd:over>
            <pd:iterationElementSlot>currentRow</pd:iterationElementSlot>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Render Data</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Write File</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Render Data</pd:from>
            <pd:to>Write File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Render Data">
            <pd:type>com.tibco.plugin.parse.RenderActivity</pd:type>
            <pd:resourceType>ae.activities.RenderActivity</pd:resourceType>
            <pd:x>629</pd:x>
            <pd:y>109</pd:y>
            <config>
                <ParseSharedConfig>/Resources/CSV/Data Format.sharedparse</ParseSharedConfig>
            </config>
            <pd:inputBindings>
                <Rows>
                    <ns:Users>
                        <ns:id>
                            <xsl:value-of select="concat('&quot;',$currentRow/ns:Users/ns:id,'&quot;')"/>
                        </ns:id>
                        <ns:firstName>
                            <xsl:value-of select="concat('&quot;',$currentRow/ns:Users/ns:firstName,'&quot;')"/>
                        </ns:firstName>
                        <ns:lastName>
                            <xsl:value-of select="concat('&quot;',$currentRow/ns:Users/ns:lastName,'&quot;')"/>
                        </ns:lastName>
                        <ns:nationality>
                            <xsl:value-of select="concat('&quot;',$currentRow/ns:Users/ns:nationality,'&quot;')"/>
                        </ns:nationality>
                        <ns:department>
                            <xsl:value-of select="concat('&quot;',$currentRow/ns:Users/ns:department,'&quot;')"/>
                        </ns:department>
                    </ns:Users>
                </Rows>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Write File">
            <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
            <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
            <pd:x>737</pd:x>
            <pd:y>102</pd:y>
            <config>
                <encoding>text</encoding>
                <compressFile>None</compressFile>
                <append>true</append>
            </config>
            <pd:inputBindings>
                <ns2:WriteActivityInputTextClass>
                    <fileName>
                        <xsl:value-of select="'C:\Users\tamilmar\Documents\TIBCO_Udemy_Course\Files\Input\users.csv'"/>
                    </fileName>
                    <textContent>
                        <xsl:value-of select="$Render-Data/Text"/>
                    </textContent>
                </ns2:WriteActivityInputTextClass>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Create Header Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Create Header Data</pd:from>
        <pd:to>Create CSV file with Header</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Create CSV file with Header</pd:from>
        <pd:to>Sample User Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Sample User Data</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>